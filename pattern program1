int main() {
    int n, i, j, k;

    scanf("%d", &n);

    for (i = 1; i <= n; i++) {
        
        k = (i % 2 == 0) ? 2 : 1;
        for (j = 1; j <= i; j++, k += 2) {
            printf("%d", k);
        }
        printf("\n");
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
